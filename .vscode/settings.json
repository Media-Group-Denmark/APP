{
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.folders.associations": {
    "(admin)": "secure",
    "(pages)": "views",
    "sitemaps": "docs",
    "(home)": "home",
    "[...not-found]": "error",

  },
  "material-icon-theme.folders.customClones": [
    {
      "name": "pages-files",
      "base": "src",
      "color": "light-green-500",
      "lightColor": "light-green-700",
      "folderNames": [
        "cookies",
        "kontakt",
        "omos",
        "privatliv",
        "kategori",
        "kategorier",
        "kategori/[kategori]",
        "tag",
        "tags",
        "tag/[tag]",
        "journalist",
        "journalister",
        "journalist/[journalist]",
        "findartikel",
        "artikel", 
        "artikel/[artikel]",
      ]
    },
    {
      "name": "custom-folders",
      "base": "context",
      "color": "green-700",
      "lightColor": "green-700",
      "folderNames": [
        "(article-collections)", "(information)", "(referencer)",
      ]
    },
  ],

  "folder-color.pathColors": [],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "APP/.eslintrc.json": true,
    "APP/.gitignore": true,
    "APP/next-env.d.ts": true,
    "APP/package-lock.json": true,
    "APP/tsconfig.json": true,
    "loadingscreen": true,
    "APP/postcss.config.js": true
  },
  "hide-files.files": [
    "APP/.eslintrc.json",
    "APP/.gitignore",
    "APP/next-env.d.ts",
    "APP/package-lock.json",
    "APP/tsconfig.json",
    "loadingscreen",
    "APP/postcss.config.js"
  ],
  "explorerExclude.backup": {},
  "editor.formatOnSave": true
}
